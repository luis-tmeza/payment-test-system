    "@types/node": "^24.10.9",
    "@types/jest": "^29.5.14",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
